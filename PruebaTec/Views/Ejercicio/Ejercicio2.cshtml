@model IEnumerable<Ubicacion>
@{
}

<table class="table table-dark">
    <thead>
        <tr>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>City</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ubicacion in Model)
        {
        <tr>
            <td>@ubicacion.Latitude</td>
            <td>@ubicacion.Longitude</td>
            <td>@ubicacion.City</td>
            <td>@ubicacion.City</td>
        </tr>
        }

    </tbody>
</table>